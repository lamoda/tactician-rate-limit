{
    "name": "lamoda/tactician-rate-limit",
    "description": "Tactician command bus middleware for rate limiting command execution",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Agaletskiy",
            "email": "pavel.agaletskiy@lamoda.ru"
        }
    ],
    "require": {
        "php": "^7.1",
        "league/tactician": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "davedevelopment/stiphle": "^0.9.2"
    },
    "autoload": {
        "psr-4": {
            "Lamoda\\Tactician\\RateLimit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lamoda\\Tactician\\RateLimit\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "suggest": {
        "davedevelopment/stiphle": "Allow using stiphle as a rate limiter implementation"
    }
}
